{
  "manifest_version": 2,
  "name": "Table info descriptor",
  "version": "1.1",
  "description": "Getting Information about first row and last row of table",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "content.js"
      ]
    }
  ]
}
